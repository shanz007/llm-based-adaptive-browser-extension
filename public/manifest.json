{
  "manifest_version": 3,
  "name": "LLM-based-Smart-Digital-UI-Assistant",
  "version": "1.0",
  "description": "A Chrome extension using React, ES6 modules, Jest for testing.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"]
}